<h1>School Class Form</h1>

<%= form_for @school_class do |f|%>
<%= f.label "school_class_title"%>
<%= f.text_field :title, id: "school_class_title"%>

<%= f.label "school_class_room_number"%>
<%= f.text_field :room_number, id: "school_class_room_number"%>

<%= f.submit value="Update School class"%>
<%end%>